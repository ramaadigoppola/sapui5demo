<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" controllerName="BasicFiori-Routing.controller.Detail">
	<Page
		title="{i18n>detailPageTitle}"
		showNavButton="true"
		navButtonPress="onNavBack">
		<core:Icon src="sap-icon://physical-activity" size="8rem"
			class="sapUiMediumMargin mine" />
		<ObjectHeader
			intro="{invoice>ShipperName}"
			title="{invoice>ProductName}"/>
	</Page>
</mvc:View>